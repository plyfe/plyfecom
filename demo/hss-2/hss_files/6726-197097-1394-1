   function mojo_clickthru() {
	     var OOBImg = new Image();
         if (typeof (mpOOBClickTrack) !== 'undefined') {
            try {
				OOBImg.src = mpOOBClickTrack;
			} catch (e) {
			  if (window.console && window.console.error) {
				   window.console.error(e.message);
			  }
			}
         }
   }

